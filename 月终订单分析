SELECT  o.user_id 
,o.order_number
,case p.package_type when 1 then '1V1' when 2 then '1v2' when 3 then '1V3' when 4 then '1V4' WHEN 5 THEN '1V5' WHEN 6 THEN '1V6' END 套餐属性
,p.`name`
,case o.order_type when 1 then '用户下单' when 2 then '业务员下单' when 3 then '外部导入(excel)' end 下单方式
,(case o.package_course_type when 1 then '正课套餐' when 2 then '活动套餐' when 3 then '赠送套餐' when 5 then '换购活动' when 6 then '录播课套餐' end ) as '订单类型'
,date(FROM_UNIXTIME(o.pay_time)) 支付时间
,o.pay_price 支付金额
from ol_order o LEFT JOIN ol_package p on p.id=o.package_id
WHERE  o.STATUS in (1,3) and o.package_course_type=1
and o.pay_time BETWEEN UNIX_TIMESTAMP('2019-04-01') and UNIX_TIMESTAMP('2019-05-01')
